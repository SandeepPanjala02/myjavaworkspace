
public class MyFirst {
	   public static void main(String[] args) {  
	         MyFirst obj = new MyFirst();  
	 }  
	 static int a = 10;  
	 static int n;  
	 int b = 5;  
	 int c;  
	 public MyFirst() {  
	       System.out.println(a + ", " + b + ", " + c + ", " + n + ", " + m);  
	   }  
	// Instance Block  
	  {  
	     b = 30;  
	     n = 20;  
	  }   
	// Static Block  
	  static   
	  {  
          a = 60;  
     }   
 }  


